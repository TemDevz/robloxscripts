while wait(1) do
for i,v in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do
if v:FindFirstChild("GunStates") then
local a =  require(v.GunStates)
a.Spread = 0
a.FireRate = 0
a.MaxAmmo = math.huge
a.Damage = math.huge
a.ReloadTime = 0
a.Range = math.huge
a.StoredAmmo = math.huge
else
return v
end
end
end
