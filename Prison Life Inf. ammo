for i, v in next, debug.getregistry() do 
    if type(v) == "table" then 
        if v.Bullets then 
            v.AutoFire = true
            v.FireRate = 0
            v.Bullets = 20
			v.Range = math.huge
            v.MaxAmmo = math.huge
			v.CurrentAmmo = math.huge
			v.StoredAmmo = math.huge
        end
    end
end
